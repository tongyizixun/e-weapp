.e-icon {
  display: inline-block;
  font-family: "iconfont";
  speak-as: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  text-rendering: auto;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  vertical-align: middle;
}
@font-face {
  font-family: "iconfont";
  /* Project id 3034328 */
  src: url("data:font/woff2;base64,") format("woff2"), url("data:font/woff;base64,") format("woff"), url("data:font/ttf;base64,") format("truetype");
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.e-success:before {
  content: "\e72d";
}
.e-fail:before {
  content: "\ed1a";
}
.e-warning:before {
  content: "\e637";
}
.e-shangla:before {
  content: "\e609";
}
.e-xiala:before {
  content: "\e6b9";
}


